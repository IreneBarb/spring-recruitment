{
    "secrets": [
        {
            "regex": "AWS[\\s|_]ACCESS[\\s|_]KEY[\\s|_]ID[\\s|_]([A-Z0-9]{20})",
            "description": "AWS Access Key ID"
        },
        {
            "regex": "AWS[\\s|_]SECRET[\\s|_]ACCESS[\\s|_]KEY[\\s|_]([A-Za-z0-9/+=]{40})",
